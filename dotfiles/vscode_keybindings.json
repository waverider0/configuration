[
  {"key": "ctrl+tab", "command": "workbench.action.nextEditor"},
  {"key": "ctrl+shift+tab", "command": "workbench.action.previousEditor"},
  {"key": "ctrl+w", "command": "-extension.vim_ctrl+w", "when": "editorTextFocus && vim.active && vim.use<C-w> && !inDebugRepl"},

  {"key": "ctrl+p", "command": "workbench.action.quickOpen"},
  {"key": "ctrl+c", "command": "workbench.action.closeQuickOpen", "when": "inQuickOpen"},
  {"key": "ctrl+n", "command": "workbench.action.quickOpenSelectNext", "when": "inQuickOpen"},
  {"key": "ctrl+p", "command": "workbench.action.quickOpenSelectPrevious", "when": "inQuickOpen"},

  {"key": "ctrl+n", "command": "selectNextSuggestion", "when": "suggestWidgetVisible && editorTextFocus"},
  {"key": "ctrl+p", "command": "selectPrevSuggestion", "when": "suggestWidgetVisible && editorTextFocus"},

  {"key": "ctrl+b", "command": "workbench.action.toggleSidebarVisibility"}
]
